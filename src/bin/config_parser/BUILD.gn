visibility = [ "$cobalt_root/*" ]

copy("test_stable_config_parser") {
  sources = [
    "test_stable_config_parser.py",
  ]
  outputs = [
    "$root_out_dir/tests/other/{{source_file_part}}",
  ]
}

group("tests") {
  testonly = true

  deps = [
    ":test_stable_config_parser",
    "src:tests",
  ]
}
